{
    "name": "Outrun Contrast",
    "type": "dark",
    "tokenColors": [
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#E9C46A"
            }
        },
        {
            "name": "Keywords",
            "scope": "keyword.control",
            "settings": {
                "foreground": "#50E191"
            }
        },
        {
            "name": "Comentários",
            "scope": "comment",
            "settings": {
                "foreground": "#909CC3"
            }
        }, 
        {
            "name": "Nome do conteúdo importado",
            "scope": "meta.import",
            "settings": {
                "foreground": "#e5e5e5"
            }
        },
        {
            "name": "Variáveis keyword",
            "scope": "storage.type",
            "settings": {
                "foreground": "#50E191"
            }
        },
        {
            // FIXME: change color
            "name": "Nome da Variável",
            "scope": ["variable.other.constant", "meta.definition.variable"],
            "settings": {
                "foreground": "#fff"
            }
        },
        {
            "name": "Function keyword",
            "scope": ["storage.type.function"],
            "settings": {
                "foreground": "#E9C46A",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Nome da função",
            "scope": ["entity.name.function"],
            "settings": {
                "foreground": "#AB90FF"
            }
        },
        {
            "name": "Nome da função",
            "scope": ["support.type.primitive"],
            "settings": {
                "foreground": "#50E191"
            }
        },
        {
            "name": "Keywords especial [async, private readonly].ts",
            "scope": ["storage.modifier"],
            "settings": {
                "foreground": "#FF916E"
            }
        },
        {
            "name": "Paramentros da função",
            "scope": ["variable.parameter"],
            "settings": {
                "foreground": "#FF916E"
            }
        },
        {
            "name": "Tipagem [.ts]",
            "scope": ["meta.type.annotation"],
            "settings": {
                "foreground": "#63ACFF"
            }
        },
        {
            "name": "Nome da interface [.ts]",
            "scope": ["entity.name.type.interface"],
            "settings": {
                "foreground": "#E9C46A"
            }
        },
        {
            "name": "Nome da interface [.ts]",
            "scope": ["meta.type.annotation"],
            "settings": {
                "foreground": "#50E191"
            }
        },
       {
            "name": "Nome do paramentro de um generico [.ts]",
            "scope": ["meta.type.parameters"],
            "settings": {
                "foreground": "#50E191"
            }
        },
        {
            "name": "Paramentros da função",
            "scope": ["meta.return.type"],
            "settings": {
                "foreground": "#FF916E"
            }
        },
        {
            "name": "Keyword try catch",
            "scope": ["keyword.control.trycatch"],
            "settings": {
                "foreground": "#FF916E"
            }
        },
