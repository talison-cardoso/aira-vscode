{
	"$schema": "vscode://schemas/color-theme",
	"name": "Aira Theme",
	"type": "dark",
	"colors": {
		// https://code.visualstudio.com/docs/getstarted/theme-color-reference
		"editor.background": "#131C24",
		"editor.foreground": "#EEFFFF",
		"activityBarBadge.background": "#FF916E",
		"sideBarTitle.foreground": "#bbbbbb",
		// Activity Bar.
		"activityBar.background": "#131C24",
		"activityBar.border": "#1C2834",
		"activityBar.foreground": "#FFFFFF", // icon
		"activityBar.inactiveForeground": "#909CC3", // icons
		"activityBarBadge.foreground": "#131C24", // number
		"activityBar.activeBorder": "#909CC3",
		"activityBar.activeBackground": "#1C2834",
		// SideBar
		"sideBar.background": "#131C24",
		"sideBar.border": "#1C2834",
		"sideBar.foreground": "#FFFFFFCC",
		"sideBarSectionHeader.background": "#131C24",
		"sideBarSectionHeader.foreground": "#FFFFFFCC",
		"sideBarSectionHeader.border": "#1C2834",
		// Button
		"button.background": "#50E191CC",
		"button.foreground": "#1C2834",
		// "button.hoverBackground": "#50E191",
		"button.secondaryBackground": "#E9C46A",
		"button.secondaryForeground": "#1C2834",
		// "button.secondaryHoverBackground": "#E9C46A",
		//	
		"descriptionForeground": "#AAA",
		"selection.background": "#ffffff4d",
		// Dropdown.
		"dropdown.background": "#131C24",
		"dropdown.border": "#111",
		"dropdown.foreground": "#FFFFFFCC",
		//welcome
		"textLink.activeForeground": "#FFFFFFCC",
		"textLink.foreground": "#FF916E",
		// Editor
		// Errors and warnings:
		"editorError.foreground": "#E63946",
		"editorWarning.foreground": "#E9C46A",
		// TODO: Debug Icons.
		"editor.selectionBackground": "#FFFFFF33",
		"editor.wordHighlightBackground": "#FFF3", // Same words other places.
		"editor.wordHighlightStrongBackground": "#FFF3", // Cursor inside current variable.
		"editor.wordHighlightStrongBorder": "#FFFC",
		"editor.lineHighlightBackground": "#131C2444", // Current line of code [background].
		"editor.lineHighlightBorder": "#131C2488",
		"editorLink.activeForeground": "#909CC3",
		"editorIndentGuide.background": "#50E19133",
		"editorIndentGuide.activeBackground": "#50E19188",
		// Numeric line	
		"editorGutter.addedBackground": "#35AD68",
		"editorGutter.deletedBackground": "#E63946",
		"editorGutter.modifiedBackground": "#E9C46A",
		// linha 123
	},
	"tokenColors": [
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#909CC3"
			}
		},
		{
			"name": "Constant Numeric",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#50E191"
			}
		},
		{
			"name": "Entity Name Class/Type",
			"scope": [
				"entity.name.class",
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#FF916E"
			}
		},
		{
			"name": "Entity Name Function",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#C8B6FF"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#50E191"
			}
		},
		{
			"name": "Keyword Operator",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#E9C46A"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#909CC3"
			}
		},
		{
			"name": "Punctuation Definition Comment",
			"scope": [
				"punctuation.definition.comment",
				"punctuation.end.definition.comment",
				"punctuation.start.definition.comment"
			],
			"settings": {
				"foreground": "#909CC3"
			}
		},
		{
			"name": "Punctuation Section",
			"scope": "punctuation.section",
			"settings": {
				"foreground": "#50E191"
			}
		},
		{
			"name": "Punctuation Section Embedded",
			"scope": [
				"punctuation.section.embedded.begin",
				"punctuation.section.embedded.end"
			],
			"settings": {
				"foreground": "#50E191"
			}
		},
		{
			"name": "Punctuation Terminator",
			"scope": "punctuation.terminator",
			"settings": {
				"foreground": "#909CC3"
			}
		},
		{
			"name": "Punctuation Variable",
			"scope": "punctuation.definition.variable",
			"settings": {
				"foreground": "#909CC3"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"foreground": "#50E191"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#FF916E"
			}
		},
		{
			"name": "String Regexp",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#00d2fa"
			}
		},
		{
			"name": "Support Class",
			"scope": "support.class",
			"settings": {
				"foreground": "#E9C46A"
			}
		},
		{
			"name": "Support Constant",
			"scope": "support.constant",
			"settings": {
				"foreground": "#43dad7"
			}
		},
		{
			"name": "{print.py} Support Function",
			"scope": "support.function",
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "{echo.php} Support Function Construct",
			"scope": "support.function.construct",
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Support Type",
			"scope": "support.type",
			"settings": {
				"foreground": "#E9C46A"
			}
		},
		{
			"name": "Support Type Exception",
			"scope": "support.type.exception",
			"settings": {
				"foreground": "#E9C46A"
			}
		},
		{
			"name": "[Storage] â€” The color for Storage Type Arrow Function",
			"scope": "storage.type.function.arrow",
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Variable",
			"scope": "variable.other",
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Variable Language",
			"scope": "variable.language",
			"settings": {
				"foreground": "#E9C46A",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Variable Parameter",
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#FFFFFFCC"
			}
		},
		{
			"name": "Color for Inherited Component",
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#E9C46A",
				"fontStyle": ""
			}
		},
		{
			"name": "[JAVASCRIPT] Color for React Extends keyword",
			"scope": [
				"storage.type.extends.js",
				"storage.type.class.jsdoc"
			],
			"settings": {
				"foreground": "#50E191"
			}
		},
		{
			"name": "[TYPESCRIPT] Color for Entity Name Type",
			"scope": "source.ts entity.name.type",
			"settings": {
				"foreground": "#FF916E"
			}
		},
		{
			"name": "[TYPESCRIPT] Color for Keyword",
			"scope": "source.ts keyword",
			"settings": {
				"foreground": "#50E191"
			}
		},
		{
			"name": "[CSS] Constant Other Color RGB Value",
			"scope": "source.css constant.other.color.rgb-value",
			"settings": {
				"foreground": "#bdbdbd"
			}
		},
		{
			"name": "[CSS](Function) Meta Property-Value",
			"scope": "source.css meta.property-value",
			"settings": {
				"foreground": "#909CC3"
			}
		},
		{
			"name": "[CSS] Media Queries",
			"scope": [
				"source.css keyword.control.at-rule.media",
				"source.css keyword.control.at-rule.media"
			],
			"settings": {
				"foreground": "#50E191"
			}
		},
		{
			"name": "[CSS] Punctuation Definition Keyword",
			"scope": "source.css punctuation.definition.keyword",
			"settings": {
				"foreground": "#909CC3"
			}
		},
		{
			"name": "[CSS] Support Type Property Name",
			"scope": "source.css support.type.property-name",
			"settings": {
				"foreground": "#ffffffbb"
			}
		},
		{
			"name": "[CSS] Entity Name Tag",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#50E191"
			}
		},
		{
			"name": "[CSS]  Entity Other Attribute Name",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#FF916E"
			}
		},
		{
			"name": "[Markdown] - Markup Bold",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
	]
}